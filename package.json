{
	"name": "notation",
	"displayName": "notation",
	"description": "自动插入jsDoc注释，主要是为了解析object类型参数",
	"version": "0.0.2",
	"publisher": "adrienld",
	"repository": {
		"type": "git",
		"url": "https://github.com/adrien-LD/notation.git"
	},
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:notation.insertNotation"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "notation.insertNotation",
				"title": "insert notation"
			}
		],
		"keybindings": [
			{
				"command": "notation.insertNotation",
				"key": "ctrl+F10",
				"mac": "ctrl+F10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus && resourceLangId == javascriptreact || editorFocus && resourceLangId == javascript",
					"command": "notation.insertNotation",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@types/fs-extra": "^9.0.1",
		"fs-extra": "^9.0.1"
	}
}
